<script>

    var jqcontact = $.noConflict();
    jqcontact('#SendEmail').click(function () {
        //if (jqcontact('#avia_8_1').val() == 3)
        saveContactus();
        //else
        //alert('please provide valid captcha.');
    });

    function saveContactus() {
        var str = jqcontact('.wpcf7-form').serialize();
        //alert(str);

        jqcontact.ajax({
            type: "POST",
            url: "/Home/SendFeedback",
            //data: {'str':str},
            data: str,
            success: function (msg) {
                //alert(msg);
                if (msg == '1') {
                    alert('Rating Added successfully');
                } else {
                    alert(msg);
                }

            },
            error: function (jqXHR, textStatus, errorThrown) {
                alert(errorThrown);
            }
        });
    }
</script>

